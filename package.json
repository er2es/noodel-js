{
  "name": "noodel",
  "version": "2.0.3",
  "license": "MIT",
  "scripts": {
    "test": "vue-cli-service serve",
    "build-base": "vue-cli-service build --target lib --name noodel src/main/Noodel.ts",
    "build-full": "vue-cli-service build --target lib --inline-vue --name noodel-full --formats umd,umd-min --no-clean src/main/Noodel.ts",
    "build": "npm run build-base && npm run build-full && rimraf dist/demo.html dist/noodel-full.css",
    "lint": "vue-cli-service lint"
  },
  "main": "dist/noodel.common.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "src",
    "types"
  ],
  "author": {
    "name": "Zuohao (Jonny) Lu"
  },
  "description": "Intuitive, adaptable swipe-based user interface for content trees",
  "dependencies": {
    "core-js": "^3.6.4",
    "css-element-queries": "^1.2.3",
    "hammerjs": "^2.0.8",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/hammerjs": "^2.0.36",
    "@types/mocha": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-typescript": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-typescript": "^5.0.1",
    "chai": "^4.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "mocha": "^8.1.1",
    "rimraf": "^3.0.2",
    "typescript": "~3.7.5",
    "vue-template-compiler": "^2.6.11"
  }
}
